@prefix : <http://example.org/aec-drawing-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/aec-drawing-ontology#> .

<http://example.org/aec-drawing-ontology#Ontology> rdf:type owl:Ontology ;
                                                    rdfs:comment "Ontology for AEC drawing elements - facade, structural, and drawing metadata. Based on schema rev02 (06.04.23)." ;
                                                    rdfs:label "AEC Drawing Ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://example.org/aec-drawing-ontology#labellableRoot
:labellableRoot rdf:type owl:AnnotationProperty ;
                rdfs:range xsd:boolean ;
                rdfs:comment "When true, this class can be used as a label by annotators (solid contour in diagram). When false, non-labellable (dashed contour)." ;
                rdfs:label "Labellable root" .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/aec-drawing-ontology#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:domain owl:Thing ;
          rdfs:range owl:Thing ;
          rdfs:comment "Direct containment or has-part relationship. Min cardinality 0 by default (can contain)." ;
          rdfs:label "contains" .


###  http://example.org/aec-drawing-ontology#hasFunction
:hasFunction rdf:type owl:ObjectProperty ;
             rdfs:domain :FacadeComponent ;
             rdfs:range :Function ;
             rdfs:comment "Entity has a functional property" ;
             rdfs:label "has function" .


###  http://example.org/aec-drawing-ontology#hasMaterial
:hasMaterial rdf:type owl:ObjectProperty ;
             rdfs:domain :FacadeComponent ;
             rdfs:range :Material ;
             rdfs:comment "Entity has a material property" ;
             rdfs:label "has material" .


###  http://example.org/aec-drawing-ontology#hasProperty
:hasProperty rdf:type owl:ObjectProperty ;
             rdfs:domain owl:Thing ;
             rdfs:range owl:Thing ;
             rdfs:comment "Entity is characterized by a property" ;
             rdfs:label "has property" .


###  http://example.org/aec-drawing-ontology#subsetOf
:subsetOf rdf:type owl:ObjectProperty ;
          rdfs:domain owl:Thing ;
          rdfs:range owl:Thing ;
          rdfs:comment "Classification or sub-typing relationship" ;
          rdfs:label "subset of" .


